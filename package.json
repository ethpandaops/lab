    "zod": "4.1.12"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4.1.1",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@eslint/css": "0.12.0",
    "@eslint/js": "9.37.0",
    "@hey-api/openapi-ts": "0.85.1",
    "@storybook/addon-docs": "9.1.10",
    "@storybook/addon-onboarding": "9.1.10",
